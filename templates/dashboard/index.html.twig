                            </div>
                        </div>
                    {% endblock %}
                {% endembed %}
            </div>

            <div class="col-12">
                {% embed '@Tabler/embeds/card.html.twig' with {margin_bottom : 0} %}
                    {% from '@Tabler/components/badge.html.twig' import badge %}
                    {% block box_attributes %}style="height: 28rem"{% endblock %}
                    {% block box_body_class %}card-body-scrollable card-body-scrollable-shadow{% endblock %}
                    {% block box_body %}
                        <div>
                            <div class="divide-y">
                                {% for index, contributor in topContributors %}
                                    <div class="row">
                                        <div class="col-auto">
                                            <span
                                                    class="avatar"
                                                    style="background-image: url({{ contributor.avatarUrl ~'&s=60' }})"
                                            ></span>
                                        </div>
                                        <div class="col">
                                            <div class="text-truncate"><strong>{{ contributor.login }}</strong>
                                            </div>
                                            <div class="text-secondary">
                                                Contributions: {{ contributor.contributions }}</div>
                                        </div>
                                        {% if index < 3 %}
                                            {% set place = index + 1 %}
                                            {% if place is same as 1 %}
                                                {% set color = 'yellow-lt' %}
                                            {% elseif place is same as 2 %}
                                                {% set color = 'gray-lt' %}
                                            {% else %}
                                                {% set color = 'orange-lt' %}
                                            {% endif %}
                                            <div class="col-auto align-self-center">
                                                {{ badge({
                                                    content: '#' ~ place,
                                                    color : color,
                                                }) }}
                                            </div>
                                        {% endif %}
                                    </div>
                                {% endfor %}
                            </div>
                        </div>
                    {% endblock %}
                {% endembed %}
            </div>
        </div>
